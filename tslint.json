{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts"
      ]
    },
    "jsRules": {},
    "rules": {
      "no-console": [false],
      "ordered-imports": [false],
      "object-literal-sort-keys": [false],
      "interface-over-type-literal": false


    },
    "rulesDirectory": []
}
